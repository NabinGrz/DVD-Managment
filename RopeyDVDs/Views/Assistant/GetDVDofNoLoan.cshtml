
@{
    ViewData["Title"] = "GetDVDofNoLoan";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>List of all DVD titles in the shop where no copy of the
                    title has been loaned in the last 31 days</h1>

<table class="table table-custom text-light mt-3">
    <thead class="bg-custom">
        <tr>
            <th>DVD Title Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.dvd)
        {
            <tr>
                <td>@item.dvdTitle.DVDTitleName</td>

                @*     <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.id">Delete</a>*@

            </tr>
        }
    </tbody>
</table>

